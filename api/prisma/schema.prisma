generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Payable {
  id           String   @id
  value        Float
  emissionDate DateTime
  assignor     Assignor @relation(fields: [assignorId], references: [id])
  assignorId   String
}

model Assignor {
  id       String    @id
  document String
  email    String
  phone    String
  name     String
  Payable  Payable[]
}

model User {
  id       String @id
  username String @unique
  password String
}
